<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="dao.DetalleFactura" table="DETALLEFACTURA">
        <id column="IDDETALLEFACTURA" name="IdDetalleFactura" type="int">
            <generator class="assigned"></generator>
        </id>
        <property name="idencabezadofactura">
            <column name="IDENCABEZADOFACTURA"></column>
        </property>
        <property name="idproducto">
            <column name="IDPRODUCTO"></column>
        </property>
        <property name="cantidad">
            <column name="CANTIDAD"></column>
        </property>
    </class>
</hibernate-mapping>
